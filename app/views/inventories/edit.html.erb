<h2> Edit Inventory</h2>

<%= form_tag "/inventories/#{@inventory.id}", method: :patch do %>

  <div>name:<input type="text" name="name" value=<%=@inventory.name%>></div>
  <div> Description:<input type="text" name="description" value=<%=@inventory.description%>></div>
  <div>start:<input type="date" name="start" value=<%=@inventory.start%>></div>
  <div>finish:<input type="date" name="finish" value=<%=@inventory.finish%>></div>
  <input type="submit" value="Update" ></input>
<% end %>
